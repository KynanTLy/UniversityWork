procedure main() returns integer {
    var out = std_output();
    integer i= 0;
    integer x = 1;
    loop while i < 2{
            'c' -> out;
            integer y = 0;
            loop while x < 4{
                'a' -> out;
                '\n' -> out;
                loop while y < 3{
                    'b' -> out;
                    y = y + 1;
                }
                '\n' -> out;
                x = x + 1;
            }
            i = i + 1;
    }



    return 0;
}