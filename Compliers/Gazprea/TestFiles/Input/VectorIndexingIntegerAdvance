procedure main() returns integer {
var out = std_output();

integer vector x = [ 1 , 2, 3 ];

integer z = x[x[2]];
z -> out;

integer y = [1,3,4][2];
y -> out;

integer q = [1,3,4][1+1];
q -> out;


integer m = [1,3,4][ x[2] ];
m -> out;


var n = [1,3,4][ x[2] ];           //Type inf
n -> out;

return 0;
}