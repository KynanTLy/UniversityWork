procedure main() returns integer {
var out = std_output();
integer vector x = [1, 1, 1, 1, 1];
integer vector y = [2, 2, 2, 2, 2];
vector v = x || y;
v -> out;
'\n' -> out;

real vector rx = [1.0, 2.3];
real vector ry = [6.7, 4.3, 2.0];
vector rv = rx || ry;
rv -> out;
'\n' -> out;

character vector cx = ['b', 'a', 'l', 'l'];
character vector cy = [' ', 's', 'a', 'c', 'k', '.'];
vector cv = cx || cy;
cv -> out;
'\n' -> out;

boolean vector bx = [true, false];
boolean vector by = [true, false, false, true];
vector bv = bx || by;
bv -> out;
'\n' -> out;

return 0;
}