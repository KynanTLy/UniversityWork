procedure main() returns integer {
var out = std_output();

integer vector x = [ 1 , 2, 3 ];

integer z = x[1];
z -> out;

x[2] ->out;
x[3]->out;


integer i = 1;
loop while i <= 3 {
    x[i] -> out;
    i = i + 1;
}


real vector r = [ 1.3 , 3.2, 3.1 ];
real r1 = r[1];
r1->out;

character vector c = [ 'a' , 'b' , 'c' ];
character c1 = c[1];
c1->out;

boolean vector b = [ true ,false, true ];
boolean b1 = b[1];
b1->out;



integer vector g = [ 1 , 2, 3 ];
var l = g[1];                           //inf type
l -> out;

real vector f = [ 1.3 , 2.1, 3.4 ];
var m = f[1];                           //inf type
m -> out;

return 0;
}